@using System.IO
@using FS.TemplateEngine
@{
    var m = Model as ComDbInfos;
    var t = m.Table;
    var proString = @"/// <summary>
        /// {2}
        /// </summary>
        public {0} {1} {3}
";
}
//===================================================
//  Copyright @@Fallstar  2017
//  作者：Fallstar
//  时间：@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")
//  说明：
//===================================================
using System;
using System.Collections.Generic;
namespace @m.NameSpace
{
    /// <summary>
    /// @t.Description
    ///</summary>
    public partial class @t.TableName
    {
    @{
        foreach (var item in t.Fileds)
        {
        <text>@string.Format(proString, item.DataTypeEx, item.FieldName, item.Description, "{ get; set; }")</text>
    }
}
    }
}
