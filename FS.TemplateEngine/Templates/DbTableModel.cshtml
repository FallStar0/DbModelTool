@using System.IO
@using FS.TemplateEngine
@{
    var m = Model as ComDbInfos;
    var t = m.Table;
    var proString = @"          /// <summary>
        /// {2}
        /// </summary>
        public {0} {1} {3}
";
}
//===================================================
//  Copyright @@ @TemplateExtraData.Copyright  @DateTime.Now.Year
//  作者：@TemplateExtraData.Author
//  时间：@DateTime.Now.ToString("yyyy-MM-dd")
//  说明：
//===================================================
using System;
namespace @m.NameSpace
{
    /// <summary>
    /// @t.Description
    ///</summary>
    public partial class @t.TableName
    {
    @{
        foreach (var item in t.Fileds)
        {
            @HtmlHelper.Raw(string.Format(proString, item.DataTypeEx, item.FieldName, item.Description, "{ get; set; }"))
        }
        }
    }
}
